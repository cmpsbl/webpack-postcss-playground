{
  "name": "webpack-postcss-playground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "serve": "better-npm-run serve",
    "serve:modules": "better-npm-run serve:modules",
    "compile": "better-npm-run compile",
    "compile:modules": "better-npm-run compile:modules"
  },
  "betterScripts": {
    "serve": {
      "command": "nodemon --ignore src/ --exec babel-node config/server",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development"
      }
    },
    "serve:modules": {
      "command": "nodemon --ignore src/ --exec babel-node config/server",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "development",
        "CSS_ENV": "css-modules"
      }
    },
    "compile": {
      "command": "npm run clean && nodemon -e js,css --exec babel-node config/compiler",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "compile:modules": {
      "command": "npm run clean && nodemon -e js,css --exec babel-node config/compiler",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production",
        "CSS_ENV": "css-modules"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "better-npm-run": "0.0.8",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.3",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.19.0",
    "lost": "^6.8.0",
    "nodemon": "^1.9.2",
    "postcss-calc": "^5.2.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-functions": "^2.1.0",
    "postcss-loader": "^0.9.1",
    "postcss-modular-scale": "^2.5.2",
    "postcss-modules-values": "^1.1.3",
    "postcss-nested": "^1.0.0",
    "postcss-strip-units": "^1.2.0",
    "postcss-vertical-rhythm": "^1.1.5",
    "postcss-vertical-rhythm-function": "^0.3.2",
    "precss": "^1.4.0",
    "rimraf": "^2.5.2",
    "rucksack-css": "^0.8.6",
    "style-loader": "^0.13.1",
    "stylelint": "^6.4.1",
    "stylelint-config-standard": "^7.0.0",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  }
}
